                                    <canvas id="appointment-status-chart"></canvas>
                                </div>
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-12 col-md-6 col-lg-7 col-xl-7">
						<div class="card">
							<div class="card-header">
								<h4 class="card-title d-inline-block">Upcoming Appointments number <%= appointment%></h4> <a href="/appointment" class="btn btn-primary float-right">View all</a>
							</div>
							<div class="card-body p-0">
								<div class="table-responsive">
									<table class="table mb-0">
										<thead class="d-none">
											<tr>
												<th>Patient Name</th>
												<th>Doctor Name</th>
												<th>Timing</th>
												
											</tr>
										</thead>
										<tbody>
                                            <% for (var i = 0; i < applist.length; i++){ %>
											<tr>
												<td style="min-width: 200px;">
													<h5 class="time-title p-0">Patient Name</h5>
													<p><%= applist[i].patient_name%></p>
												</td>                 
												<td>
													<h5 class="time-title p-0">Appointment With</h5>
													<p><%= applist[i].doctor_name%></p>
												</td>
												<td>
													<h5 class="time-title p-0">Timing</h5>
													<p><%= applist[i].time%></p>
												</td>
												<td class="text-right">
													<a href="/appointment" class="btn btn-primary btn-sm take-btn">Take up</a>
                                                </td>
                                                <% } %>
											</tr>
											
											
											
										
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
                    <div class="col-12 col-md-6 col-lg-5 col-xl-5">
