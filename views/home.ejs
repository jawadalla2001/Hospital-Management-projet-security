                                    <i class="fa fa-user-md"></i>
                                </div>
                                <div class="dash-card-content">
                                    <h2 id="doctors-count"><%= doc %></h2>
                                    <div class="dash-card-trend">
                                        <span>Total Doctors</span>
                                        <span id="doctors-trend" class="ml-2"><i class="fa fa-minus text-muted"></i> 0</span>
                                    </div>
                                </div>
                            </div>
                            <div class="dash-card-footer">
                                <a href="/doctors">View all doctors</a>
                            </div>
                        </div>
                    </div>
