                            <div class="card-header">
                                <h5 class="card-title">Quick Actions</h5>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-3 col-sm-6">
                                        <a href="/appointment/add_appointment" class="quick-action" data-action="new-appointment">
                                            <div class="quick-action-card bg-info-light">
                                                <i class="fa fa-calendar-plus-o text-info"></i>
                                                <span>New Appointment</span>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="col-md-3 col-sm-6">
                                        <a href="#" class="quick-action" data-action="new-patient">
                                            <div class="quick-action-card bg-success-light">
                                                <i class="fa fa-user-plus text-success"></i>
                                                <span>New Patient</span>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="col-md-3 col-sm-6">
                                        <a href="/compose" class="quick-action" data-action="send-message">
                                            <div class="quick-action-card bg-warning-light">
                                                <i class="fa fa-envelope-o text-warning"></i>
                                                <span>Send Message</span>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="col-md-3 col-sm-6">
                                        <a href="#" class="quick-action" data-action="generate-report">
                                            <div class="quick-action-card bg-danger-light">
                                                <i class="fa fa-file-pdf-o text-danger"></i>
                                                <span>Generate Report</span>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Key Performance Indicators -->
