        <div class="page-wrapper">
            <div class="content">
                <!-- Dashboard Header with Date Filter -->
                <div class="row mb-4">
                    <div class="col-md-6">
                        <h4 class="page-title">Dashboard</h4>
                        <div class="text-muted mt-1">Hospital overview and statistics</div>
                    </div>
                    <div class="col-md-6 text-right">
                        <div class="btn-group">
                            <button id="refresh-dashboard" class="btn btn-white mr-2" data-toggle="tooltip" title="Refresh Data">
                                <i class="fa fa-refresh"></i>
                            </button>
                            <div id="dashboard-date-range" class="btn btn-white">
                                <i class="fa fa-calendar mr-2"></i>
                                <span></span>
                                <i class="fa fa-caret-down ml-2"></i>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Loading Overlay -->
                <div id="dashboard-loading" class="dashboard-loading d-none">
                    <div class="spinner-border text-primary" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>

                <!-- Success Message -->
                <div id="refresh-success" class="alert alert-success fade show d-none" role="alert">
                    Dashboard data refreshed successfully.
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>

                <!-- Quick Action Cards -->
