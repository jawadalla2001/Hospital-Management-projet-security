    <script>
        // Initialize medicine inventory features
        $(document).ready(function() {
            // View toggle (Table/Card)
            $('.view-toggle-btn').on('click', function() {
                const view = $(this).data('view');
                $('.view-toggle-btn').removeClass('active');
                $(this).addClass('active');
                
                if (view === 'table') {
                    $('#table-view').show();
                    $('#card-view').hide();
                } else {
                    $('#table-view').hide();
                    $('#card-view').show();
                }
            });

            // Toggle filters panel
            $('#toggle-filters').on('click', function() {
                $(this).find('i').toggleClass('fa-angle-down fa-angle-up');
                $(this).closest('.card').find('.card-body').slideToggle();
            });

            // Select all medicines
            $('#select-all').on('change', function() {
                $('.medicine-checkbox').prop('checked', $(this).prop('checked'));
                updateBatchActionVisibility();
            });

            // Individual checkbox change
            $('.medicine-checkbox').on('change', function() {
                updateBatchActionVisibility();
            });

            // Calculate dashboard metrics
            calculateDashboardMetrics();
        });

        // Update batch actions visibility
        function updateBatchActionVisibility() {
            const selectedCount = $('.medicine-checkbox:checked').length;
            $('.selected-count').text(selectedCount);
            
            if (selectedCount > 0) {
                $('#batch-actions').slideDown();
            } else {
                $('#batch-actions').slideUp();
            }
        }

        // Calculate dashboard metrics
        function calculateDashboardMetrics() {
            let lowStock = 0;
            let expiringSoon = 0;
            let totalValue = 0;
            
            // Loop through all medicines
            $('tr[data-id], .medicine-card').each(function() {
                const stockLevel = $(this).data('stock-level');
                const expiryStatus = $(this).data('expiry-status');
                const price = parseFloat($(this).data('price') || 0);
                const quantity = parseInt($(this).data('quantity') || 0);
                
                if (stockLevel === 'low') {
                    lowStock++;
                }
                
                if (expiryStatus === 'expiring-soon') {
                    expiringSoon++;
                }
                
                totalValue += price * quantity;
            });
            
            // Update dashboard
            $('#low-stock').text(lowStock);
            $('#expiring-soon').text(expiringSoon);
            $('#inventory-value').text(totalValue.toFixed(2));
        }

        // Show medicine details
        function showMedicineDetails(id) {
            const row = $(`tr[data-id="${id}"], .medicine-card[data-id="${id}"]`);
            
            // Populate modal with medicine data
            $('#detail-name').text(row.data('name'));
            $('#detail-id').text(`Medicine ID: ${id}`);
            $('#detail-price').text(row.data('price'));
            $('#detail-quantity').text(row.data('quantity'));
            $('#detail-purchase-date').text(row.data('purchase-date'));
            $('#detail-expiry-date').text(row.data('expiry-date'));
            
            // Set stock level progress
            const stockLevel = row.data('stock-level');
            const stockPercentage = stockLevel === 'low' ? 25 : stockLevel === 'medium' ? 50 : 90;
            const stockClass = stockLevel === 'low' ? 'bg-danger' : stockLevel === 'medium' ? 'bg-warning' : 'bg-success';
            
            $('#detail-stock-progress')
                .css('width', `${stockPercentage}%`)
                .removeClass('bg-danger bg-warning bg-success')
                .addClass(stockClass)
                .text(`${stockPercentage}%`);
                
            // Toggle warning visibility
            if (stockLevel === 'low') {
                $('#detail-warning').show();
            } else {
                $('#detail-warning').hide();
            }
            
            // Set edit link
            $('#detail-edit-link').attr('href', `/store/edit_med/${id}`);
            
            // Show modal
            $('#medicine_details').modal('show');
        }

        // Export medicine list
        function exportMedicineList(format) {
            // This would be implemented with actual export functionality
            alert(`Exporting medicine list as ${format}...`);
        }
    </script>
