                    </ul>
                </div>
            </div>
        </div>
        <div class="page-wrapper">
            <div class="content">
                <!-- Page Header -->
                <div class="row mb-4">
                    <div class="col-md-7">
                        <h4 class="page-title">Pharmacy Inventory</h4>
                        <div class="text-muted mt-1">Manage and track hospital medicine inventory</div>
                    </div>
                    <div class="col-md-5 text-right">
                        <div class="btn-group view-toggle mt-2">
                            <button type="button" class="btn btn-white view-toggle-btn active" data-view="table">
                                <i class="fa fa-table"></i> Table View
                            </button>
                            <button type="button" class="btn btn-white view-toggle-btn" data-view="card">
                                <i class="fa fa-th"></i> Card View
                            </button>
                        </div>
                        <a href="/store/add_med" class="btn btn-primary btn-rounded ml-2"><i class="fa fa-plus"></i> Add Medicine</a>
                    </div>
                </div>
                
                <!-- Inventory Stats Dashboard -->
                <div class="row">
                    <div class="col-md-3 col-sm-6">
                        <div class="dash-widget clearfix card-box">
                            <span class="dash-widget-icon"><i class="fa fa-cubes"></i></span>
                            <div class="dash-widget-info">
                                <h3 id="total-medicines"><%= list.length %></h3>
                                <span>Total Medicines</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="dash-widget clearfix card-box">
                            <span class="dash-widget-icon bg-warning"><i class="fa fa-calendar-times-o"></i></span>
                            <div class="dash-widget-info">
                                <h3 id="expiring-soon">0</h3>
                                <span>Expiring Soon</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="dash-widget clearfix card-box">
                            <span class="dash-widget-icon bg-danger"><i class="fa fa-exclamation-triangle"></i></span>
                            <div class="dash-widget-info">
                                <h3 id="low-stock">0</h3>
                                <span>Low Stock</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="dash-widget clearfix card-box">
                            <span class="dash-widget-icon bg-info"><i class="fa fa-money"></i></span>
                            <div class="dash-widget-info">
                                <h3 id="inventory-value">0.00</h3>
                                <span>Inventory Value</span>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- Search & Filters Panel -->
                <div class="card mb-4">
                    <div class="card-header">
                        <h5 class="card-title">Search & Filters</h5>
                        <a href="javascript:void(0);" class="btn btn-sm btn-link" id="toggle-filters">
                            <i class="fa fa-angle-down"></i>
                        </a>
                    </div>
                    <div class="card-body">
                        <form action="/store/search" method="POST" id="medicine-search-form">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Search</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fa fa-search"></i></span>
                                            </div>
                                            <input type="text" class="form-control filter-input" id="medicine-search" name="search" placeholder="Search by name, category...">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Category</label>
                                        <select class="form-control filter-input" id="category-filter" name="category">
                                            <option value="all">All Categories</option>
                                            <option value="antibiotics">Antibiotics</option>
                                            <option value="analgesics">Analgesics</option>
                                            <option value="antiseptics">Antiseptics</option>
                                            <option value="antipyretics">Antipyretics</option>
                                            <option value="vitamins">Vitamins</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Stock Level</label>
                                        <select class="form-control filter-input" id="stock-filter" name="stock">
                                            <option value="all">All Stock Levels</option>
                                            <option value="low">Low Stock</option>
                                            <option value="medium">Medium Stock</option>
                                            <option value="high">High Stock</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Expiry Status</label>
                                        <select class="form-control filter-input" id="expiry-filter" name="expiry">
                                            <option value="all">All Status</option>
                                            <option value="expired">Expired</option>
                                            <option value="expiring-soon">Expiring Soon</option>
                                            <option value="valid">Valid</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Sort By</label>
                                        <select class="form-control filter-input" id="sort-select" name="sort">
                                            <option value="name-asc">Name (A-Z)</option>
                                            <option value="name-desc">Name (Z-A)</option>
                                            <option value="price-asc">Price (Low-High)</option>
                                            <option value="price-desc">Price (High-Low)</option>
                                            <option value="expiry-asc">Expiry Date</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="text-right">
                                <button type="button" id="clear-filters" class="btn btn-outline-secondary mr-2">Clear Filters</button>
                                <button type="submit" class="btn btn-primary">Apply Filters</button>
                            </div>
                        </form>
                    </div>
                </div>
                
                <!-- Batch Actions Toolbar -->
                <div id="batch-actions" class="row mb-3" style="display: none;">
                    <div class="col-12">
                        <div class="card card-box p-3">
                            <div class="d-flex justify-content-between align-items-center">
                                <div>
                                    <span class="text-muted">Selected</span>
                                    <span class="badge badge-primary ml-1 selected-count">0</span>
                                </div>
                                <div class="btn-group">
                                    <button type="button" class="btn btn-outline-danger btn-sm" id="batch-delete">
                                        <i class="fa fa-trash-o"></i> Delete Selected
                                    </button>
                                    <button type="button" class="btn btn-outline-warning btn-sm" id="batch-print-labels">
                                        <i class="fa fa-print"></i> Print Labels
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- Action Toolbar -->
                <div class="row mb-3">
                    <div class="col-12">
                        <div class="btn-group">
                            <button type="button" class="btn btn-white" onclick="exportMedicineList('csv')">
                                <i class="fa fa-file-excel-o"></i> Export
                            </button>
                            <button type="button" class="btn btn-white" onclick="exportMedicineList('print')">
                                <i class="fa fa-print"></i> Print
                            </button>
                        </div>
                    </div>
                </div>
                <!-- Table View -->
                <div class="row" id="table-view">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-body p-0">
                                <div class="table-responsive">
                                    <table class="table table-striped custom-table" id="medicines-table">
                                        <thead>
                                            <tr>
                                                <th style="width: 30px;">
                                                    <div class="custom-control custom-checkbox">
                                                        <input type="checkbox" class="custom-control-input" id="select-all">
                                                        <label class="custom-control-label" for="select-all"></label>
                                                    </div>
                                                </th>
                                                <th>ID</th>
                                                <th>Name</th>
                                                <th>Purchase Date</th>
                                                <th>Expire</th>
                                                <th>Expire End</th>
                                                <th>Price</th>
                                                <th>Quantity</th>
                                                <th>Status</th>
                                                <th class="text-right">Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (var i = 0; i < list.length; i++){ %>
                                            <tr data-id="<%= list[i].id %>" 
                                                data-name="<%= list[i].name %>" 
                                                data-price="<%= list[i].price %>" 
                                                data-quantity="<%= list[i].quantity %>" 
                                                data-category="generic" 
                                                data-purchase-date="<%= list[i].p_date %>" 
                                                data-expiry-date="<%= list[i].expire_end %>"
                                                data-stock-level="<%= parseInt(list[i].quantity) < 20 ? 'low' : parseInt(list[i].quantity) < 50 ? 'medium' : 'high' %>"
                                                data-expiry-status="valid">
                                                <td>
                                                    <div class="custom-control custom-checkbox">
                                                        <input type="checkbox" class="custom-control-input medicine-checkbox" id="check-<%= list[i].id %>" value="<%= list[i].id %>">
                                                        <label class="custom-control-label" for="check-<%= list[i].id %>"></label>
                                                    </div>
                                                </td>
                                                <td><strong><%= list[i].id %></strong></td>
                                                <td><a href="#" onclick="showMedicineDetails('<%= list[i].id %>')"><%= list[i].name %></a></td>
                                                <td><%= list[i].p_date %></td>
                                                <td><%= list[i].expire %> years</td>
                                                <td class="expiry-date" data-date="<%= list[i].expire_end %>"><%= list[i].expire_end %></td>
                                                <td><%= list[i].price %></td>
                                                <td class="text-center">
                                                    <span class="badge stock-level <%= parseInt(list[i].quantity) < 20 ? 'bg-danger' : parseInt(list[i].quantity) < 50 ? 'bg-warning' : 'bg-success' %>">
                                                        <%= list[i].quantity %>
                                                    </span>
                                                </td>
                                                <td>
                                                    <span class="custom-badge status-green">Active</span>
                                                </td>
                                                <td class="text-right">
                                                    <div class="dropdown dropdown-action">
                                                        <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                                        <div class="dropdown-menu dropdown-menu-right">
                                                            <a class="dropdown-item" href="/store/edit_med/<%= list[i].id %>"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                            <a class="dropdown-item" href="/store/delete_med/<%= list[i].id %>" ><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                                                            <a class="dropdown-item" href="#" onclick="showMedicineDetails('<%= list[i].id %>')"><i class="fa fa-eye m-r-5"></i> View Details</a>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- Card View -->
                <div class="row" id="card-view" style="display: none;">
                    <% for (var i = 0; i < list.length; i++){ %>
                    <div class="col-md-4 col-sm-6 col-12 mb-4">
                        <div class="card medicine-card" 
                             data-id="<%= list[i].id %>" 
                             data-name="<%= list[i].name %>" 
                             data-price="<%= list[i].price %>" 
                             data-quantity="<%= list[i].quantity %>" 
                             data-category="generic" 
                             data-purchase-date="<%= list[i].p_date %>" 
                             data-expiry-date="<%= list[i].expire_end %>"
                             data-stock-level="<%= parseInt(list[i].quantity) < 20 ? 'low' : parseInt(list[i].quantity) < 50 ? 'medium' : 'high' %>"
                             data-expiry-status="valid">
                            <div class="card-body">
                                <div class="d-flex justify-content-between mb-3">
                                    <div>
                                        <h5 class="card-title text-primary mb-0"><%= list[i].name %></h5>
                                        <div class="text-muted small">ID: <%= list[i].id %></div>
                                    </div>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input medicine-checkbox" id="card-check-<%= list[i].id %>" value="<%= list[i].id %>">
                                        <label class="custom-control-label" for="card-check-<%= list[i].id %>"></label>
                                    </div>
                                </div>
                                <div class="medicine-details mb-3">
                                    <div class="row no-gutters">
                                        <div class="col-6 mb-1">
                                            <span class="text-muted">Price:</span>
                                        </div>
                                        <div class="col-6 mb-1 text-right">
                                            <%= list[i].price %>
                                        </div>
                                        <div class="col-6 mb-1">
                                            <span class="text-muted">Stock:</span>
                                        </div>
                                        <div class="col-6 mb-1 text-right">
                                            <span class="badge <%= parseInt(list[i].quantity) < 20 ? 'badge-danger' : parseInt(list[i].quantity) < 50 ? 'badge-warning' : 'badge-success' %>">
                                                <%= list[i].quantity %>
                                            </span>
                                        </div>
                                        <div class="col-6 mb-1">
                                            <span class="text-muted">Expiry:</span>
                                        </div>
                                        <div class="col-6 mb-1 text-right">
                                            <%= list[i].expire_end %>
                                        </div>
                                    </div>
                                </div>
                                <div class="medicine-actions text-center">
                                    <a href="/store/edit_med/<%= list[i].id %>" class="btn btn-sm btn-outline-primary" data-action="edit"><i class="fa fa-pencil"></i></a>
                                    <a href="#" class="btn btn-sm btn-outline-info" onclick="showMedicineDetails('<%= list[i].id %>')" data-action="view"><i class="fa fa-eye"></i></a>
                                    <a href="#" class="btn btn-sm btn-outline-success" data-action="reorder"><i class="fa fa-refresh"></i></a>
                                    <a href="/store/delete_med/<%= list[i].id %>" class="btn btn-sm btn-outline-danger" data-action="delete"><i class="fa fa-trash-o"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% } %>
                                </tbody>
                        </div>
                    </div>
                </div>
            </div>
            
            <!-- Medicine Details Modal -->
            <div class="modal fade" id="medicine_details" tabindex="-1" role="dialog">
                <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="medicine-modal-title">Medicine Details</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="medicine-info mb-4">
                                        <h4 id="detail-name">Loading...</h4>
                                        <div class="text-muted" id="detail-id"></div>
                                    </div>
                                    
                                    <div class="row mb-4">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Price</label>
                                                <div id="detail-price" class="form-control-static"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Current Stock</label>
                                                <div id="detail-quantity" class="form-control-static"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Purchase Date</label>
                                                <div id="detail-purchase-date" class="form-control-static"></div>
                                            </div>
                                        </div>
